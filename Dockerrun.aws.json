{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"rushildayabme/meetAPP-client",
            "hostname":"client",
            "essential":false,
            "memory": 128
        },
        {
            "name":"api",
            "image":"rushildayabme/meetAPP-api",
            "hostname":"api",
            "essential":false,
            "memory": 128
        },
        {
            "name":"nginx",
            "image":"rushildayabme/meetAPP-nginx",
            "hostname":"nginx",
            "essential":true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links": [
                "client",
                "api"
            ]
        }
    ]
}